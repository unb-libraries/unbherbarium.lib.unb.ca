{
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Project definition for unbherbarium.ca",
    "extra": {
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "Views/Date/Exposed filters fix.": "https://www.drupal.org/files/issues/2811887-2-exposed-date-restore-type-ealier.patch"
            },
            "drupal/search_api": {
                "Avoid disappearing fields.": "https://www.drupal.org/files/issues/2792277-4--respect_syncing_state.patch"
            }
        }
    },
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "name": "unblibraries/unbherbarium.ca",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "drupal/config_update": "1.1",
        "drupal/console": "~1.0",
        "drupal/core": "8.2.3",
        "drupal/features": "3.1",
        "drupal/field_collection": "1.0-alpha1",
        "drupal/field_group": "1.0-rc6",
        "drupal/search_api": "1.0-beta3",
        "drupal/views_autocomplete_filters": "1.1",
        "drush/drush": "~8.0"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "drupal/devel": "1.0-alpha1",
        "drupal/drupal-extension": "~3.0",
        "drupal/migrate_plus": "3.0-beta1",
        "drupal/migrate_source_csv": "2.0-beta1",
        "drupal/migrate_tools": "3.0-beta1",
        "drupal/migrate_upgrade": "3.0-rc1",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
