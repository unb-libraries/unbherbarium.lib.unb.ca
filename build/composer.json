{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": "source"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Project definition for unbherbarium.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/taxonomy_manager": {
                "Taxonomy Manager Invalid Placeholder": "https://www.drupal.org/files/issues/invalid_place_holder_issue-2681969-2.patch",
                "Taxonomy Manager JSON Load Error": "https://www.drupal.org/files/issues/2673054-load-error-11.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "unblibraries/unbherbarium.lib.unb.ca",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/jackmoore/colorbox/archive/master.zip"
                },
                "name": "jackmoore/colorbox",
                "require": {
                    "composer/installers": "~1.0"
                },
                "type": "drupal-library",
                "version": "master"
            },
            "type": "package"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://malsup.github.io/jquery.cycle.all.js"
                },
                "name": "malsup/jquery.cycle",
                "require": {
                    "composer/installers": "^1.0.20"
                },
                "type": "drupal-library",
                "version": "master"
            },
            "type": "package"
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.6.1/chosen_v1.6.1.zip"
                },
                "name": "harvesthq/chosen",
                "require": {
                    "composer/installers": "^1.2.0"
                },
                "type": "drupal-library",
                "version": "1.6.1"
            },
            "type": "package"
        },
        {
            "type": "git",
            "url": "https://github.com/unb-libraries/drupal-dev-libs.git"
        }
    ],
    "require": {
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "~1.6.1",
        "drupal-composer/drupal-scaffold": "~2.2.0",
        "drupal/bootstrap": "3.5",
        "drupal/chosen": "2.3",
        "drupal/colorbox": "1.4",
        "drupal/config_update": "1.3",
        "drupal/console": "~1.0",
        "drupal/core": "8.3.3",
        "drupal/csv_serialization": "1.0",
        "drupal/features": "3.5",
        "drupal/field_group": "1.0-rc6",
        "drupal/google_analytics": "2.1",
        "drupal/pathauto": "1.0",
        "drupal/search_api": "1.1",
        "drupal/search_api_solr": "1.0-rc4",
        "drupal/simple_gmap": "1.3",
        "drupal/simplify": "1.0",
        "drupal/taxonomy_access_fix": "2.1",
        "drupal/taxonomy_manager": "1.x-dev",
        "drupal/token": "1.0",
        "drupal/tvi": "1.0-beta1",
        "drupal/twig_field_value": "1.1",
        "drupal/views_autocomplete_filters": "1.1",
        "drupal/views_slideshow": "4.5",
        "drush/drush": "~8.1",
        "harvesthq/chosen": "1.6.*",
        "jackmoore/colorbox": "master",
        "solarium/solarium": "~3.8",
        "webflo/drupal-finder": "^0.3.0",
        "webmozart/path-util": "^2.3",
        "drupal/views_rest_feed": "1.0-alpha1"
    },
    "require-dev": {
        "unblibraries/drupal-dev-libs": "master"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
