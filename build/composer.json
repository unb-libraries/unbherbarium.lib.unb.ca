{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of unbherbarium.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "unb-libraries/unbherbarium.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://malsup.github.io/jquery.cycle.all.js"
                },
                "name": "malsup/jquery.cycle",
                "require": {
                    "composer/installers": "^1.0.20"
                },
                "type": "drupal-library",
                "version": "1.0"
            },
            "type": "package"
        },
        {
            "type": "package",
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip"
                },
                "name": "harvesthq/chosen",
                "require": {
                    "composer/installers": "^1.2.0"
                },
                "type": "drupal-library",
                "version": "1.8.7"
            }
        }
    ],
    "require": {
        "drupal/bootstrap": "3.23",
        "drupal/chosen": "2.9",
        "drupal/config_update": "1.7",
        "drupal/core": "8.9.2",
        "drupal/csv_serialization": "1.4",
        "drupal/editor_file": "1.5",
        "drupal/features": "3.11",
        "drupal/field_group": "3.1",
        "drupal/google_analytics": "2.5",
        "drupal/health_check": "1.2",
        "drupal/honeypot": "1.30",
        "drupal/migrate_plus": "4.2",
        "drupal/migrate_source_csv": "2.2",
        "drupal/migrate_tools": "4.5",
        "drupal/pathauto": "1.8",
        "drupal/redis": "1.4",
        "drupal/search_api": "1.17",
        "drupal/search_api_autocomplete": "1.3",
        "drupal/search_api_solr": "4.1.3",
        "drupal/simple_entity_merge": "1.2",
        "drupal/simple_gmap": "3.0.0",
        "drupal/taxonomy_access_fix": "2.8",
        "drupal/taxonomy_manager": "1.0-alpha2",
        "drupal/token": "1.7",
        "drupal/tvi": "1.0-beta2",
        "drupal/twig_field_value": "1.2",
        "drupal/views_autocomplete_filters": "1.3",
        "drupal/views_slideshow": "4.7",
        "harvesthq/chosen": "~1.8",
        "malsup/jquery.cycle": "1.0",
        "neitanod/forceutf8": "~2.0",
        "teqneers/php-stream-wrapper-for-git": "1.0.1",
        "unb-libraries/drupal-builder": "^8.0.0"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
