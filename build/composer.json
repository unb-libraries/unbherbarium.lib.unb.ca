{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Project definition for unbherbarium.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "Views date type exposed filter.": "https://www.drupal.org/files/issues/2648950-79-8.2.x.patch"
            },
            "drupal/taxonomy_manager": {
                "Taxonomy Manager Invalid Placeholder": "https://www.drupal.org/files/issues/invalid_place_holder_issue-2681969-2.patch",
                "Taxonomy Manager JSON Load Error": "https://www.drupal.org/files/issues/2673054-load-error-11.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "unblibraries/unbherbarium.ca",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/jackmoore/colorbox/archive/master.zip"
                },
                "name": "jackmoore/colorbox",
                "require": {
                    "composer/installers": "~1.0"
                },
                "type": "drupal-library",
                "version": "master"
            },
            "type": "package"
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.6.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.6.1/chosen_v1.6.1.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "~1.6.1",
        "drupal-composer/drupal-scaffold": "~2.2.0",
        "drupal/bootstrap": "3.4",
        "drupal/chosen": "2.3",
        "drupal/colorbox": "1.4",
        "drupal/config_update": "1.3",
        "drupal/console": "~1.0",
        "drupal/core": "8.2.8",
        "drupal/csv_serialization": "1.0",
        "drupal/features": "3.5",
        "drupal/field_group": "1.0-rc6",
        "drupal/pathauto": "1.0",
        "drupal/search_api": "1.0",
        "drupal/search_api_autocomplete": "1.0-alpha1",
        "drupal/search_api_solr": "1.0-beta3",
        "drupal/simple_gmap": "1.2",
        "drupal/simplify": "1.0",
        "drupal/taxonomy_access_fix": "2.1",
        "drupal/taxonomy_manager": "1.x-dev",
        "drupal/token": "1.0",
        "drupal/twig_field_value": "1.1",
        "drupal/views_autocomplete_filters": "1.1",
        "drupal/views_slideshow": "4.4",
        "drush/drush": "~8.1",
        "webflo/drupal-finder": "^0.2.1",
        "webmozart/path-util": "^2.3",
        "harvesthq/chosen": "1.6.*",
        "heykarthikwithu/cycle": "1.1",
        "jackmoore/colorbox": "master",
        "solarium/solarium": "~3.8"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "drupal/devel": "1.0-rc2",
        "drupal/drupal-extension": "~3.0",
        "drupal/migrate_plus": "3.0-beta1",
        "drupal/migrate_source_csv": "2.0",
        "drupal/migrate_tools": "3.0-beta1",
        "drupal/migrate_upgrade": "3.0-rc1",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
