{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of unbherbarium.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "name": "unb-libraries/unbherbarium.lib.unb.ca-drupal8",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://malsup.github.io/jquery.cycle.all.js"
                },
                "name": "malsup/jquery.cycle",
                "require": {
                    "composer/installers": "^1.0.20"
                },
                "type": "drupal-library",
                "version": "1.0"
            },
            "type": "package"
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip"
                },
                "name": "harvesthq/chosen",
                "require": {
                    "composer/installers": "^1.2.0"
                },
                "type": "drupal-library",
                "version": "1.8.3"
            },
            "type": "package"
        },
        {
            "type": "git",
            "url": "https://github.com/unb-libraries/drupal-development-libraries.git"
        }
    ],
    "require": {
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "~1.6.1",
        "drupal-composer/drupal-scaffold": "~2.2.0",
        "drupal/bootstrap": "3.13",
        "drupal/chosen": "2.6",
        "drupal/config_update": "1.5",
        "drupal/console": "~1.0",
        "drupal/core": "8.6.2",
        "drupal/csv_serialization": "1.4",
        "drupal/editor_file": "1.2",
        "drupal/features": "3.8",
        "drupal/field_group": "3.0-beta1",
        "drupal/google_analytics": "2.3",
        "drupal/honeypot": "1.29",
        "drupal/migrate_plus": "4.0",
        "drupal/migrate_source_csv": "2.2",
        "drupal/migrate_tools": "4.0",
        "drupal/pathauto": "1.3",
        "drupal/search_api": "1.9",
        "drupal/search_api_solr": "1.2",
        "drupal/simple_entity_merge": "1.0",
        "drupal/simple_gmap": "1.4",
        "drupal/taxonomy_access_fix": "2.6",
        "drupal/taxonomy_manager": "1.0-alpha2",
        "drupal/token": "1.5",
        "drupal/tvi": "1.0-beta2",
        "drupal/twig_field_value": "1.1",
        "drupal/views_autocomplete_filters": "1.2",
        "drupal/views_slideshow": "4.6",
        "drush/drush": "^8.1.15",
        "harvesthq/chosen": "~1.8",
        "malsup/jquery.cycle": "1.0",
        "solarium/solarium": "~3.8",
        "teqneers/php-stream-wrapper-for-git": "1.0.1",
        "webflo/drupal-finder": "^0.3.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
